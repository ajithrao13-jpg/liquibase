# Database Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env file with real credentials to version control

# Target Database
DB_HOST=your_db_host
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_username
DB_PASS=your_password

# Reference Database (for diff/diffchangelog commands)
REF_DB_HOST=your_ref_db_host
REF_DB_PORT=5432
REF_DB_NAME=your_ref_database_name
REF_DB_USER=your_ref_username
REF_DB_PASS=your_ref_password

# PostgreSQL JDBC Driver Version
POSTGRES_JDBC_VERSION=42.7.8
